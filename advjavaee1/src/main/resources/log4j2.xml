<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} [%t] %level %logger{1} - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
        <!--<Logger name = "StaxXmlReader" level="info">-->
            <!--<AppenderRef ref="Console"/>-->
        <!--</Logger>-->
    </Loggers>
</Configuration>
